{
  "name": "preact-starter",
  "type": "module",
  "version": "1.0.0",
  "description": "Barebones starter for a preact project",
  "main": "dist/index.js",
  "scripts": {
    "build": "vite build",
    "serve": "vite preview",
    "start": "vite",
    "test": "playwright test",
    "test:debug": "playwright test --debug",
    "test:report": "playwright show-report",
    "lint": "prettier --no-error-on-unmatched-pattern --check **/*.{js,jsx,ts,tsx,css,md,html,yml,yaml,json}",
    "lint:fix": "prettier --no-error-on-unmatched-pattern --write **/*.{js,jsx,ts,tsx,css,md,html,yml,yaml,json}",
    "prepare": "husky install"
  },
  "author": "Andre Wiggins",
  "license": "MIT",
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,css,md,html,yml,yaml,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@preact/signals": "^1.2.2",
    "deepsignal": "^1.3.6",
    "imurmurhash": "^0.1.4",
    "preact": "^10.19.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@preact/preset-vite": "^2.7.0",
    "@types/node": "^20.10.4",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.1",
    "vite": "^5.0.7"
  },
  "volta": {
    "node": "20.10.0"
  }
}
